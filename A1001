#include <iostream>
#include <cstdio>
#include <string>
#include <vector>
#include <algorithm>
using namespace std;
int main() {
	freopen("input.txt", "r", stdin);
	string s1, s2;
	cin >> s1 >> s2;
	string s;
	int a, b;
	a = stoi(s1);
	b = stoi(s2);
	int temp = a + b;
	if (temp < 0) {
		printf("-");
	}
	temp = abs(temp);
	if (temp < 1000) {
		printf("%d", temp);
		return 0;
	}
	while (temp > 0) {
		int k;
		k = temp % 10;
		char ch;
		ch = k + '0';
		temp = temp / 10;
		s.push_back(ch);
	}
	int len = s.length();
	int count = 0;
	for (int i = len - 1; i >= 0; i--) {
		cout << s[i];
		count++;
		if (count == 3 && i > 0) {
			count = 0;
			printf(",");
		}
	}
	return 0;
}
